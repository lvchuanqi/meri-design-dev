{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@vue/babel-plugin-transform-vue-jsx",
    ["@babel/plugin-transform-runtime", { "corejs": { "version": 3, "proposals": true } }]
  ]
}
